<li class="wrapper">
<div class="figure bgcolor-danger"><i class="ico-heart"></i></div>
	<!-- panel -->
	<div class="panel">
		<div class="panel-body">
			<ul class="list-table">
				<li class="text-left" style="width:50px;">
					<img class="img-circle" src='<%= cloudinary_url( result[0].default_pic, :width => 50, :height => 50, :crop => :fill, :gravity => :faces) %>' alt="<%= result[0].name %>" >
				</li>
				<li class="text-left">
					<p class="nm"><span class="semibold"><%= result[0].name %></span> likes to follow <span class="semibold"><%= result[2].name %></span></p>
				</li>
				<li class="text-right" style="width:50px;">
					<img class="img-circle" src='<%= cloudinary_url( result[2].default_pic, :width => 50, :height => 50, :crop => :fill, :gravity => :faces) %>' alt="<%= result[2].name %>" >
				</li>
			</ul>
		</div>
	</div>
	<!--/ panel -->
</li>